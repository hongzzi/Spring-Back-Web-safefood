<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sql.menuMapper">
	<select id="getWeekdayMenuList" parameterType="map" resultType="menu">
		select *
		  from menu_info
		 where menu_date between #{startDate} and #{endDate}
	</select>
</mapper>
